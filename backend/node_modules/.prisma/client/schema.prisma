generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Puppy {
  pet_id                Int     @id @default(autoincrement())
  name                  String
  breed                 String?
  age_est               Int?
  current_kennel_number Int?
}
